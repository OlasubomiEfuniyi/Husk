#include "stack.h"

void init_stack(Stack *stack, int num_elems) {
  if(stack != NULL) {
    if((stack->data = calloc(num_elems, sizeof(void *))) == NULL) {
      perror("Memory allocation failed");
    }
    stack->top = 0;
  }
}

